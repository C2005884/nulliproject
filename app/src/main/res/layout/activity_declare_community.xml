<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".declare.DeclareCommunityActivity">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:text="커뮤니티 이용"
        android:textColor="@color/black"
        android:textSize="20dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="70dp"
        android:gravity="left"
        android:layout_marginLeft="30dp"
        android:textSize="15dp"
        android:text="게시판 이용"
        android:textStyle="bold"
        android:textColor="@color/black"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"/>

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="110dp"
        android:gravity="left"
        android:layout_marginLeft="30dp"
        android:textSize="15dp"
        android:text="좋아요 취소는 어떻게 하나요?"
        android:textStyle="normal"
        android:textColor="@color/txt_dark_gray"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"/>

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="150dp"
        android:gravity="left"
        android:layout_marginLeft="30dp"
        android:textSize="15dp"
        android:text="스크랩 취소는 어떻게 하나요?"
        android:textStyle="normal"
        android:textColor="@color/txt_dark_gray"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"/>

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="190dp"
        android:gravity="left"
        android:layout_marginLeft="30dp"
        android:textSize="15dp"
        android:text="사진을 첨부하려는데, 사진이 보이지 않아요."
        android:textStyle="normal"
        android:textColor="@color/txt_dark_gray"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"/>

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="230dp"
        android:gravity="left"
        android:layout_marginLeft="30dp"
        android:textSize="15dp"
        android:text="글이 삭제되면 스크랩한 글도 지워지나요?"
        android:textStyle="normal"
        android:textColor="@color/txt_dark_gray"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"/>

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="350dp"
        android:gravity="left"
        android:layout_marginLeft="30dp"
        android:textSize="15dp"
        android:text="등록한 장애물 정보를 수정할 수 있나요?"
        android:textStyle="normal"
        android:textColor="@color/txt_dark_gray"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"/>

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="310dp"
        android:gravity="left"
        android:layout_marginLeft="30dp"
        android:textSize="15dp"
        android:text="등록한 후기 정보를 수정할 수 있나요?"
        android:textStyle="normal"
        android:textColor="@color/txt_dark_gray"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"/>

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="270dp"
        android:gravity="left"
        android:layout_marginLeft="30dp"
        android:textSize="15dp"
        android:text="삭제된 글/댓글을 복구할 수 있나요?"
        android:textStyle="normal"
        android:textColor="@color/txt_dark_gray"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"/>



    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="420dp"
        android:gravity="left"
        android:layout_marginLeft="30dp"
        android:textSize="15dp"
        android:text="게시판 관리"
        android:textStyle="bold"
        android:textColor="@color/black"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"/>

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="460dp"
        android:gravity="left"
        android:layout_marginLeft="30dp"
        android:textSize="15dp"
        android:text="타인이 적은 게시물을 삭제하고 싶어요."
        android:textStyle="normal"
        android:textColor="@color/txt_dark_gray"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"/>

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="500dp"
        android:gravity="left"
        android:layout_marginLeft="30dp"
        android:textSize="15dp"
        android:text="제 개인정보가 적혀있는 게시물을 삭제하고 싶어요."
        android:textStyle="normal"
        android:textColor="@color/txt_dark_gray"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"/>

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="540dp"
        android:gravity="left"
        android:layout_marginLeft="30dp"
        android:textSize="15dp"
        android:text="명예훼손 및 유해정보의 소지가 있는 게시물을 삭제하고 싶어요."
        android:textStyle="normal"
        android:textColor="@color/txt_dark_gray"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"/>

</androidx.constraintlayout.widget.ConstraintLayout>